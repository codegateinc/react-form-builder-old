{
  "name": "react-form-builder",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir dist --extensions \".ts,.tsx\"",
    "build:watch": "rimraf dist && tsc --watch",
    "tsc": "node_modules/typescript/bin/tsc",
    "tsc:watch": "node_modules/typescript/bin/tsc --watch",
    "test": "react-scripts-ts test --env=jsdom --coverage",
    "test:cov": "react-scripts-ts test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls",
    "start": "react-scripts-ts start"
  },
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codegateinc/react-form-builder.git"
  },
  "keywords": [
    "react",
    "react form builder"
  ],
  "author": "CodeGate",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "bugs": {
    "url": "https://github.com/codegateinc/react-form-builder/issues"
  },
  "homepage": "https://github.com/codegateinc/react-form-builder#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.16",
    "babel-plugin-module-resolver": "^3.2.0",
    "jsdom": "15.1.0",
    "ramda": "0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts-ts": "4.0.8",
    "tslint": "5.17.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "@types/ramda": "^0.26.33",
    "styled-components": "4.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/index.ts"
    ]
  }
}
